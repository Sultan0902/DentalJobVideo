<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_privacy_policy"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/bg"
    tools:context="com.vishant.DentalJobVideo.activity.PrivacyPolicyActivity">
    <include
        android:id="@+id/top_header_layout"
        layout="@layout/top_header_layout">
    </include>


    <WebView
        android:id="@+id/privacy_policy_webview"
        android:layout_marginTop="@dimen/header_layout_topmargin"
        android:layout_below="@+id/top_header_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</RelativeLayout>
